        "type": "input",
        "value": "Restart",
        "display": "hide"
      },
      "SuccessInterval": {
        "type": "input",
        "value": 0,
        "display": "hide"
      },
      "FailureInterval": {
        "type": "input",
        "value": 120,
        "display": "hide"
      },
      "ServerUpdate": {
        "type": "input",
        "value": "05:00",
        "display": "hide"
      }
    },
    "Storage": {
      "Storage": {
        "type": "storage",
        "value": {},
        "valuetype": "ignore",
        "display": "disabled"
      }
    }
  },
